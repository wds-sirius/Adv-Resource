            "EpisodeMasterId": 1030404,
            "Order": 1,
            "GroupOrder": 10,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "フフッ、それは向こう脛のことだろう？",
            "VoiceFileName": "10304040100",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 11,
                    "LipSyncMasterId": 1,
