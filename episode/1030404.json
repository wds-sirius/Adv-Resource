        },
        {
            "Id": 1030404016,
            "EpisodeMasterId": 1030404,
            "Order": 1,
            "GroupOrder": 16,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "銀河座にスカウトされた時に、過去の公演を調べてな。/n『橋弁慶』も伝統ある演目だったので少々調べたまでだ。",
            "VoiceFileName": "10304040160",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 15,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                },
                {
                    "slotNumber": 2,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 4,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
