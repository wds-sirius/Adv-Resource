        },
        {
            "Id": 1030404037,
            "EpisodeMasterId": 1030404,
            "Order": 1,
            "GroupOrder": 37,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "情感深い、弁慶と牛若丸の人間模様。/nそして迫力ある殺陣――",
            "VoiceFileName": "10304040370",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
