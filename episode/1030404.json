        {
            "Id": 1030404011,
            "EpisodeMasterId": 1030404,
            "Order": 1,
            "GroupOrder": 11,
            "SpeakerName": "緋花里",
            "FontSize": 2,
            "Phrase": "そうそう！　足の脛！/n弁慶と言えば、そこが弱点だったって有名だよね！",
            "VoiceFileName": "10304040110",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                },
                {
                    "slotNumber": 2,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 10,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 4,
