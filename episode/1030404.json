            "EpisodeMasterId": 1030404,
            "Order": 1,
            "GroupOrder": 8,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "物語の中心人物の名は、武蔵坊弁慶――",
            "VoiceFileName": "10304040080",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 21,
                    "LipSyncMasterId": 1,
