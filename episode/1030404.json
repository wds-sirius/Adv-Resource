            "EpisodeMasterId": 1030404,
            "Order": 1,
            "GroupOrder": 30,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "こほんっ。/nそれでは、出来る限り簡潔にお話しいたしますわね。",
            "VoiceFileName": "10304040300",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 70,
                    "BodyMotionMasterId": 11,
                    "LipSyncMasterId": 1,
                    "SpineId": 30101,
