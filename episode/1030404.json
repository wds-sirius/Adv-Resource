        },
        {
            "Id": 1030404036,
            "EpisodeMasterId": 1030404,
            "Order": 1,
            "GroupOrder": 36,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "弁慶にはそういった逸話も残されているのです。/nそちらは、『鬼若丸乃鯉退治』という名で知られていますわね。",
            "VoiceFileName": "10304040360",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "BodyMotionMasterId": 21,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
