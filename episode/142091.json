        },
        {
            "Id": 142091015,
            "EpisodeMasterId": 142091,
            "Order": 1,
            "GroupOrder": 15,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "緋花里を案じる気持ちは分かる。/n舞台の方針に対する不満もな。",
            "WindowEffect": 1,
            "VoiceFileName": "1420910130",
            "CharacterMotions": [
