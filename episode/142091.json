            "EpisodeMasterId": 142091,
            "Order": 1,
            "GroupOrder": 20,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "（……なのに私はあの時、/n　皆さんを纏め上げることができませんでしたわ）",
            "VoiceFileName": "1420910170",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 400,
                    "BodyMotionMasterId": 25,
                    "SpineId": 30101,
