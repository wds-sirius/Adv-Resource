            "EpisodeMasterId": 142091,
            "Order": 1,
            "GroupOrder": 21,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "（あのまま舞台に立ったとしても、/n　上手くいかないことは明白でしたのに）",
            "VoiceFileName": "1420910180",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 73,
                    "SpineId": 30101,
