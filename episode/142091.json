            "Order": 1,
            "GroupOrder": 12,
            "SpeakerName": "銀河座団員達",
            "FontSize": 2,
            "Phrase": "……何もあんな言い方しなくてもいいのに。",
            "WindowEffect": 1,
            "VoiceFileName": "1420910100",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
