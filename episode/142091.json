            "SpeakerIconId": "302"
        },
        {
            "Id": 142091016,
            "EpisodeMasterId": 142091,
            "Order": 1,
            "GroupOrder": 16,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "だが先生にも先生の、銀河座を守る運営としての考えがある。/n立場を理解することが肝要だ。",
            "WindowEffect": 1,
            "VoiceFileName": "1420910140",
