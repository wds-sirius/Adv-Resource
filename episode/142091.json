            "EpisodeMasterId": 142091,
            "Order": 1,
            "GroupOrder": 26,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "（元々、手放しで喜べるような立場でもありませんけれど。/n　『千寿暦』は、完璧であることが求められているのですから……）",
            "VoiceFileName": "1420910230",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 73,
                    "BodyMotionMasterId": 13,
                    "SpineId": 30101,
                    "CharacterAppearanceType": 0,
