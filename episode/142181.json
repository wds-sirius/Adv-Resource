        {
            "Id": 142181006,
            "EpisodeMasterId": 142181,
            "Order": 1,
            "GroupOrder": 6,
            "SpeakerName": "緋花里",
            "FontSize": 2,
            "Phrase": "あ、あの……！/n大丈夫？　どうかした……？",
            "VoiceFileName": "1421810060",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
