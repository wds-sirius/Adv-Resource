            "SpeakerIconId": "201"
        },
        {
            "Id": 142241022,
            "EpisodeMasterId": 142241,
            "Order": 1,
            "GroupOrder": 22,
            "SpeakerName": "Eden劇団員",
            "FontSize": 2,
            "Phrase": "はい、ありがとうございます！/nではすぐに、準備に取り掛かります。",
            "VoiceFileName": "1422410210",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
