            "EpisodeMasterId": 142212,
            "Order": 1,
            "GroupOrder": 17,
            "SpeakerName": "叶羽",
            "FontSize": 2,
            "Phrase": "（みんなでやりたいことやって、楽しいが一番って感じで。/n　確かにこんな時期だけど……それは大事にしないとじゃん）",
            "VoiceFileName": "1422120160",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 71,
                    "SpineId": 40501,
                    "CharacterAppearanceType": 0,
