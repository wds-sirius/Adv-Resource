            "SpeakerIconId": "405"
        },
        {
            "Id": 142212014,
            "EpisodeMasterId": 142212,
            "Order": 1,
            "GroupOrder": 14,
            "SpeakerName": "叶羽",
            "FontSize": 2,
            "Phrase": "（自身のセンスとして表れるほどの役への思い入れ……、/n　憑依型らしい、演技の迫力……）",
            "VoiceFileName": "1422120130",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 100,
                    "BodyMotionMasterId": 11,
                    "SpineId": 40501,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
