            "EpisodeMasterId": 1030207,
            "Order": 1,
            "GroupOrder": 33,
            "SpeakerName": "元劇団員C",
            "FontSize": 2,
            "Phrase": "そっか、アレをやるんだ。これは楽しみだね。",
            "VoiceFileName": "10302070320",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
