        {
            "Id": 1030207006,
            "EpisodeMasterId": 1030207,
            "Order": 1,
            "GroupOrder": 6,
            "SpeakerName": "倫子",
            "FontSize": 2,
            "Phrase": "気にしないで。今となってはいい思い出だよ。/nてゆーか銀河座じゃ２本しかやれなかったしね。",
            "VoiceFileName": "10302070060",
            "CharacterMotions": [
                {
