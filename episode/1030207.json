            "SpeakerIconId": "301"
        },
        {
            "Id": 1030207033,
            "EpisodeMasterId": 1030207,
            "Order": 1,
            "GroupOrder": 32,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "次回公演は、馴染みの深い伝統の演目。/n銀河座の輝きは、天上の印となりましょう。",
            "VoiceFileName": "10302070310",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 100,
                    "HeadMotionMasterId": 1,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
