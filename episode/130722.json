            "EpisodeMasterId": 130722,
            "Order": 1,
            "GroupOrder": 20,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "ですが、ダイヤさんのみならずルビィさんにまで――",
            "VoiceFileName": "1307220200",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 72,
                    "BodyMotionMasterId": 11,
                    "LipSyncMasterId": 4,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
