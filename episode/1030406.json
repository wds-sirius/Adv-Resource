        },
        {
            "Id": 1030406024,
            "EpisodeMasterId": 1030406,
            "Order": 1,
            "GroupOrder": 24,
            "SpeakerName": "銀河座劇団員D",
            "FontSize": 2,
            "Phrase": "マクベス夫人の演技を見ちゃったら、納得の配役だよねー。",
            "VoiceFileName": "10304060240",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1030406025,
            "EpisodeMasterId": 1030406,
            "Order": 1,
            "GroupOrder": 25,
            "SpeakerName": "リリヤ",
            "FontSize": 2,
            "Phrase": "（弁慶と牛若丸に対峙する、鬼……。/n　それなら、誰よりも美しく恐ろしい鬼を追求するだけ）",
            "VoiceFileName": "10304060250",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 71,
                    "BodyMotionMasterId": 14,
                    "SpineId": 30403,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
