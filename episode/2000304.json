        {
            "Id": 2000304014,
            "EpisodeMasterId": 2000304,
            "Order": 1,
            "GroupOrder": 14,
            "SpeakerName": "八恵の母",
            "FontSize": 2,
            "Phrase": "ええ。今度教会でバザーをするんですって。/nうちもお手伝いしたいと思って。もう使わなくなったものを出そうと思うの。",
            "VoiceFileName": "20003040130",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2000304015,
            "EpisodeMasterId": 2000304,
            "Order": 1,
            "GroupOrder": 15,
            "SpeakerName": "八恵の母",
            "FontSize": 2,
            "Phrase": "あなたの物もあるから、確認してくれるかしら？",
            "VoiceFileName": "20003040140",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2000304016,
            "EpisodeMasterId": 2000304,
            "Order": 1,
            "GroupOrder": 16,
            "SpeakerName": "八恵",
            "FontSize": 2,
            "Phrase": "はい……わあ、懐かしい。おままごとセット！",
            "VoiceFileName": "20003040150",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 200,
                    "BodyMotionMasterId": 5,
                    "LipSyncMasterId": 2,
                    "SpineId": 10401,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
