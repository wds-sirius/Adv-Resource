            "EpisodeMasterId": 2000304,
            "Order": 1,
            "GroupOrder": 36,
            "SpeakerName": "八恵の母",
            "FontSize": 2,
            "Phrase": "八恵、あなたもしかして……。",
            "VoiceFileName": "20003040350",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
