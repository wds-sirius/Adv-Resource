            "EpisodeMasterId": 140182,
            "Order": 1,
            "GroupOrder": 5,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "でも海かぁ。やぁば、考えただけでニヤけるっての。",
            "VoiceFileName": "1401820050",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 200,
