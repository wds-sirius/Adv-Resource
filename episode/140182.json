            "SpeakerIconId": "403"
        },
        {
            "Id": 140182008,
            "EpisodeMasterId": 140182,
            "Order": 1,
            "GroupOrder": 6,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "やっほー、突発配信だよー。/n短い間だけどよろしくー。",
            "VoiceFileName": "1401820060",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 200,
                    "BodyMotionMasterId": 3,
                    "LipSyncMasterId": 2,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
