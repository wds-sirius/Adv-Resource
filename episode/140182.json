            "EpisodeMasterId": 140182,
            "Order": 2,
            "GroupOrder": 5,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "……そだ。ついでに配信しちゃおっかな。",
            "VoiceFileName": "1401820051",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 17,
                    "LipSyncMasterId": 2,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
