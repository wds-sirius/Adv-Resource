        },
        {
            "Id": 1030407004,
            "EpisodeMasterId": 1030407,
            "Order": 1,
            "GroupOrder": 4,
            "SpeakerName": "演出チーフ",
            "FontSize": 2,
            "Phrase": "５分休憩だ。少し頭を冷やせ。",
            "VoiceFileName": "10304070040",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1030407005,
            "EpisodeMasterId": 1030407,
            "Order": 1,
            "GroupOrder": 5,
            "SpeakerName": "緋花里",
            "FontSize": 2,
            "Phrase": "はあ……。/nああ、ダメダメ！　しっかりしないと！",
            "VoiceFileName": "10304070050",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 73,
