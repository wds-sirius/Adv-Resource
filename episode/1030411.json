            "EpisodeMasterId": 1030411,
            "Order": 1,
            "GroupOrder": 29,
            "SpeakerName": "元劇団員C",
            "FontSize": 2,
            "Phrase": "言うじゃないか。",
            "VoiceFileName": "10304110280",
            "CharacterMotions": [
