            "EpisodeMasterId": 1030201,
            "Order": 1,
            "GroupOrder": 35,
            "SpeakerName": "MC",
            "FontSize": 2,
            "Phrase": "それではおふたりの歴史を紐解いていきましょうね。/nまずドイツ時代のお話を——",
            "VoiceFileName": "10302010300",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
