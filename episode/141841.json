            "EpisodeMasterId": 141841,
            "Order": 1,
            "GroupOrder": 18,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "うおう、コメント欄が海コールで一色に……！？/nそんなに期待されてるとは……。",
            "VoiceFileName": "1418410180",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 500,
                    "BodyMotionMasterId": 5,
                    "LipSyncMasterId": 2,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
