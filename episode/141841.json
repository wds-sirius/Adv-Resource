            "SpeakerIconId": "105"
        },
        {
            "Id": 141841014,
            "EpisodeMasterId": 141841,
            "Order": 1,
            "GroupOrder": 13,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "……ふむふむ。/n『都内のプールレビュー』に『祭の屋台食べ歩き』……。",
            "VoiceFileName": "1418410130",
            "CharacterMotions": [
