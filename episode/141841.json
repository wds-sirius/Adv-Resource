        },
        {
            "Id": 141841011,
            "EpisodeMasterId": 141841,
            "Order": 1,
            "GroupOrder": 10,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "まぁ、撮れ高あるかもだし、一瞬悩んだんですけど……。/n睡魔には勝てませんでした。",
            "VoiceFileName": "1418410100",
            "CharacterMotions": [
