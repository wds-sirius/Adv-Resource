            "SpeakerIconId": "105"
        },
        {
            "Id": 141841013,
            "EpisodeMasterId": 141841,
            "Order": 1,
            "GroupOrder": 12,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "あ、いっそここでみんなに聞いちゃおうかな。/n今後、ぱんだのチャンネルでどんなネタが見たいです？",
            "VoiceFileName": "1418410120",
