            "SpeakerIconId": "105"
        },
        {
            "Id": 141841012,
            "EpisodeMasterId": 141841,
            "Order": 1,
            "GroupOrder": 11,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "でもなぁ、釣りは置いとくとしても、/nそろそろ夏の配信ネタを考えないとなんですよねー。",
            "VoiceFileName": "1418410110",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 72,
                    "BodyMotionMasterId": 7,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
