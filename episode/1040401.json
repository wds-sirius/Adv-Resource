            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 11,
            "SpeakerName": "ひばり",
            "FontSize": 2,
            "Phrase": "『……そして、演劇ファンの皆様ならばもうお分かりのことでしょう！』",
            "VoiceFileName": "10404010100",
            "CharacterMotions": [],
            "SpeakerIconId": "522"
