        },
        {
            "Id": 1040401021,
            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 21,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "この『世界演劇リーグ』の審査が来ると、具体的にはどーなるわけ？/nすごい劇団って認められる感じ？",
            "VoiceFileName": "10404010200",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                },
                {
                    "slotNumber": 2,
                    "FacialExpressionMasterId": 103,
