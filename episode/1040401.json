            "SpeakerIconId": "401"
        },
        {
            "Id": 1040401020,
            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 20,
            "SpeakerName": "いろは",
            "FontSize": 2,
            "Phrase": "この間の舞台も、かなり評判良かったですからね！/nもしかしたら、もしかするかもしれないじゃないですか！",
            "VoiceFileName": "10404010190",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 202,
                    "BodyMotionMasterId": 2,
                    "LipSyncMasterId": 2,
                    "SpineId": 0,
