            ],
            "SpeakerIconId": "402"
        },
        {
            "Id": 1040401029,
            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 29,
            "SpeakerName": "叶羽",
            "FontSize": 2,
            "Phrase": "えー、世界演劇リーグの目に留まったところで、/nいいことなんて何もないっての。真面目で堅苦しくなるだけよ。",
            "VoiceFileName": "10404010280",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 110,
                    "BodyMotionMasterId": 15,
                    "LipSyncMasterId": 4,
                    "SpineId": 40503,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "405"
        },
        {
            "Id": 1040401030,
            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 30,
            "SpeakerName": "叶羽",
            "FontSize": 2,
            "Phrase": "そしたら今までみたいに、みんなで楽しく、なんて言ってられなくなるし。/nだったらこのまま、自由にゆるーくやれてるほうが、ラクでいいじゃない。",
            "VoiceFileName": "10404010290",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 71,
                    "BodyMotionMasterId": 27,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "405"
        },
        {
            "Id": 1040401031,
            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 31,
            "SpeakerName": "電姫劇団員たち",
            "FontSize": 2,
            "Phrase": "「うわっ、それは盲点だったわ……」「だったら今のままがいいかも……」/n「だね。オタトークできなくなるのは死活問題だし……」",
            "VoiceFileName": "10404010300",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1040401032,
            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 32,
            "SpeakerName": "叶羽",
            "FontSize": 2,
            "Phrase": "まっ、なんにせよ来るわけないんだし、話すだけムダムダ！/nだったらもっと別の楽しい話を――。",
            "VoiceFileName": "10404010310",
