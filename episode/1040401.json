            "SpeakerIconId": "401"
        },
        {
            "Id": 1040401024,
            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 24,
            "SpeakerName": "いろは",
            "FontSize": 2,
            "Phrase": "だから、もし審査が来たら、今よりももっといろんな人たちが、/n電姫の舞台を観に来てくれるようになると思うんです！",
            "VoiceFileName": "10404010230",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 208,
                    "BodyMotionMasterId": 2,
                    "LipSyncMasterId": 2,
                    "SpineId": 0,
