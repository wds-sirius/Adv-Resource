            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 14,
            "SpeakerName": "美兎",
            "FontSize": 2,
            "Phrase": "そ、そうなの……かな！？/nどうしよう……審査なんて、緊張しちゃうな……。",
            "VoiceFileName": "10404010130",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
