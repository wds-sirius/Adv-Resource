            "EpisodeMasterId": 1040401,
            "Order": 1,
            "GroupOrder": 10,
            "SpeakerName": "ひばり",
            "FontSize": 2,
            "Phrase": "『会場はここ東京、上野に新設されます『アストロラーべホール』！』",
            "VoiceFileName": "10404010090",
            "CharacterMotions": [],
            "SpeakerIconId": "522"
