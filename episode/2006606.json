            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 36,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "……。",
            "VoiceFileName": "20066060360",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 104,
                    "BodyMotionMasterId": 13,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
