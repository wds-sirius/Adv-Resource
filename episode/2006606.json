    "EpisodeDetail": [
        {
            "Id": 2006606001,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 1,
            "SpeakerName": "クレア",
            "FontSize": 2,
            "Phrase": "なんでしょうか、ラモーナ先輩。/n稽古場に来てくれ……なんて。",
            "BackgroundImageFileName": "670",
            "BgmFileName": "201",
            "VoiceFileName": "20066060010",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606002,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 2,
            "SpeakerName": "アビゲイル",
            "FontSize": 2,
            "Phrase": "そうそう、それも来てる面子的にワークショップに居た子ばかりじゃん。/nまたワークショップを始める気ぃ～？　なんて……。",
            "VoiceFileName": "20066060020",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606003,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 3,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "ああ、そのつもりだ。",
            "VoiceFileName": "20066060030",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "HeadMotionMasterId": 1,
                    "BodyMotionMasterId": 15,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606004,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 4,
            "SpeakerName": "クレア",
            "FontSize": 2,
            "Phrase": "えっ！？",
            "VoiceFileName": "20066060040",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606005,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 5,
            "SpeakerName": "アビゲイル",
            "FontSize": 2,
            "Phrase": "モナ、本気？",
            "VoiceFileName": "20066060050",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606006,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 6,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "当然だ、ホロスコープシーズン2度目とはいえ、/n今回の演目は配役も多く、個性的だ。",
            "VoiceFileName": "20066060060",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 101,
                    "BodyMotionMasterId": 17,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606007,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 7,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "この中で役を勝ち取ることが出来た者でも、分からないことがあるだろう。/nそれを皆で共有して、答えを探し、演技の向上につなげる。",
            "VoiceFileName": "20066060070",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "BodyMotionMasterId": 15,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606008,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 8,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "また、ワークショップを始めよう。/n前のように……な。",
            "VoiceFileName": "20066060080",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "BodyMotionMasterId": 14,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606009,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 9,
            "SpeakerName": "アビゲイル",
            "FontSize": 2,
            "Phrase": "その、モナ……大丈夫なの？",
            "VoiceFileName": "20066060090",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606010,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 10,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "解雇者のことなら大丈夫だ、発覚しようとも私が責任を取る。/n気になるようであれば、ワークショップを断ろうとも構わない。",
            "VoiceFileName": "20066060100",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 71,
                    "BodyMotionMasterId": 3,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606011,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 11,
            "SpeakerName": "アビゲイル",
            "FontSize": 2,
            "Phrase": "いや、そうじゃなくて！　モナも今、そういうことするの不味いんでしょ？/nさすがに見てればわかるよ。",
            "VoiceFileName": "20066060110",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606012,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 12,
            "SpeakerName": "クレア",
            "FontSize": 2,
            "Phrase": "その、私たちの為なら……大丈夫です。/nラモーナ先輩の足を引っ張りたくはありませんし……。",
            "VoiceFileName": "20066060120",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606013,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 13,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "そんなことはないさ。これまでもお前たちとのワークショップは、/n私にとって演技を見直すきっかけとなっていたからな。",
            "VoiceFileName": "20066060130",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 17,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606014,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 14,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "それに、私は今まで我身可愛さに迷っていた。/n自分がこの劇団に入って掲げた信念を忘れそうになっていたんだ。",
            "VoiceFileName": "20066060140",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 71,
                    "BodyMotionMasterId": 14,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606015,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 15,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "私はもう一度、自分の信念を取り戻したいんだ。/nだから、頼む……この通りだ。",
            "VoiceFileName": "20066060150",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 107,
                    "BodyMotionMasterId": 13,
                    "LipSyncMasterId": 4,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606016,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 16,
            "SpeakerName": "アビゲイル",
            "FontSize": 2,
            "Phrase": "……ふふっ、そういうことならオッケーだよ！/nちょうど、自分だけじゃ煮詰まってたんだよね～。",
            "VoiceFileName": "20066060160",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606017,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 17,
            "SpeakerName": "クレア",
            "FontSize": 2,
            "Phrase": "そうですね……！　ラモーナ先輩の意見がもらえるならありがたいです！/n……それに、ラモーナ先輩の役に立ちたいですから。",
            "VoiceFileName": "20066060170",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006606018,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 18,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "みんな……ありがとう。それじゃあワークショップを始めるぞ！/nまずは現段階でのお前たちの演技を見せてもらう！",
            "VoiceFileName": "20066060180",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 101,
                    "BodyMotionMasterId": 8,
                    "LipSyncMasterId": 2,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "302"
        },
        {
            "Id": 2006606019,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 19,
            "FontSize": 2,
            "Phrase": "",
            "BackgroundImageFileFadeType": 1,
            "BgmFileName": "203",
            "CharacterMotions": [],
            "FadeValue1": 0.20000000298023224,
            "FadeValue2": 0.30000001192092896,
