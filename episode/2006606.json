        {
            "Id": 2006606020,
            "EpisodeMasterId": 2006606,
            "Order": 1,
            "GroupOrder": 20,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "……よし、片付けも終わったな。/nふふっ……。",
            "VoiceFileName": "20066060190",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 11,
                    "LipSyncMasterId": 1,
                    "SpineId": 30203,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
