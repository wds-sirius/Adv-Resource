            "EpisodeMasterId": 120122,
            "Order": 1,
            "GroupOrder": 6,
            "SpeakerName": "いろは",
            "FontSize": 2,
            "Phrase": "ええと、ハンドルネームなし。会員No.0001。",
            "VoiceFileName": "1201220060",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 105,
