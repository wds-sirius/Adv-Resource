            "SpeakerIconId": "1"
        },
        {
            "Id": 120122018,
            "EpisodeMasterId": 120122,
            "Order": 1,
            "GroupOrder": 16,
            "SpeakerName": "いろは",
            "FontSize": 2,
            "Phrase": "……ありがとう、ゼロワンさん。/nおかげで、僕のままでいいんだと思えるよ。",
            "VoiceFileName": "1201220160",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 205,
                    "BodyMotionMasterId": 6,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
