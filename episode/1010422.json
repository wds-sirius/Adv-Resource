            "FadeValue1": 0.20000000298023224,
            "FadeValue2": 0.30000001192092896,
            "FadeValue3": 0.20000000298023224
        },
        {
            "Id": 1010422020,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 20,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "『どうやって遊ぶんだい？』",
            "VoiceFileName": "10104220170",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422021,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 21,
            "SpeakerName": "八恵",
            "FontSize": 2,
            "Phrase": "『その時、できることを。でも、わたしはオモチャを持ってないの』/n　ポニーヌもゼルマも、人形を貸してくれないから。",
            "BackgroundCharacterImageFileName": "141910_0",
            "VoiceFileName": "10104220180",
            "CharacterMotions": [],
            "SpeakerIconId": "104"
        },
        {
            "Id": 1010422022,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 22,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "（くぅ～、新妻っちのコゼットの不憫さ！/n　いいぞ、さっすー！存分に引き出せてる！）",
            "VoiceFileName": "10104220190",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
        {
            "Id": 1010422023,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 23,
            "FontSize": 2,
            "Phrase": "",
            "BackgroundImageFileName": "473",
