            "FadeValue3": 0.10000000149011612
        },
        {
            "Id": 1010422024,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 24,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "はぁ……はぁ……。/n飛ばし、すぎたかな……。",
            "VoiceFileName": "10104220200",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422025,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 25,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "（なんだか、色々考えられなくなるくらい気持ちいい。/n　身体が勝手に動いて、喉が舞台に合わせて震えて、声を出して――）",
            "VoiceFileName": "10104220210",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422026,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 26,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "（私、今一番ワクワクしてる。/n　まだまだ、輝けるんじゃないかって思ってる）",
            "VoiceFileName": "10104220220",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422027,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 27,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "次はぱんだの見せ場だ。/nマリウスの腕の中で息絶えるエポニーヌの場面。",
            "VoiceFileName": "10104220230",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422028,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 28,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "ぱんだは……。/nぁ……。",
            "BgmFileName": "999",
            "VoiceFileName": "10104220240",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422029,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 29,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "『約束してください……！』",
            "BgmFileName": "105",
            "VoiceFileName": "10104220250",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
        {
            "Id": 1010422030,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 30,
            "SpeakerName": "静香",
            "FontSize": 2,
            "Phrase": "『あ、ああ、約束する』",
            "VoiceFileName": "10104220260",
            "CharacterMotions": [],
            "SpeakerIconId": "102"
        },
        {
            "Id": 1010422031,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 31,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "『あたしが死んだら額にキスをしてくださると、/n　お約束ください……』",
            "VoiceFileName": "10104220270",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
        {
            "Id": 1010422032,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 32,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "『きっと、感じ取れる……』",
            "VoiceFileName": "10104220280",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
