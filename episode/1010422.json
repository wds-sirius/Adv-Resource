            "FadeValue1": 0.10000000149011612,
            "FadeValue2": 0.20000000298023224,
            "FadeValue3": 0.10000000149011612
        },
        {
            "Id": 1010422011,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 11,
            "SpeakerName": "カトリナ",
            "FontSize": 2,
            "Phrase": "『この浅ましい女は、市長を侮辱したのです！』",
            "VoiceFileName": "10104220090",
            "CharacterMotions": [],
            "SpeakerIconId": "103"
        },
        {
            "Id": 1010422012,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 12,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "『それは私一個人の問題です』",
            "VoiceFileName": "10104220100",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422013,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 13,
            "SpeakerName": "カトリナ",
            "FontSize": 2,
            "Phrase": "『ムッシュ・ル・メール、失礼を承知で申しますが、/nこれはあなた一個人への侮辱ではなく、法律への侮辱なのです！』",
            "VoiceFileName": "10104220110",
            "CharacterMotions": [],
            "SpeakerIconId": "103"
        },
        {
            "Id": 1010422014,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 14,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "（カトりんもやべーくらい乗ってる。/n　観客のハラハラ感が伝わってくる）",
            "VoiceFileName": "10104220120",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
        {
            "Id": 1010422015,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 15,
            "SpeakerName": "ここな",
            "FontSize": 2,
            "Phrase": "『あ……あ、あ…………！』",
            "BackgroundCharacterImageFileName": "141920_0",
            "VoiceFileName": "10104220130",
            "CharacterMotions": [],
            "SpeakerIconId": "101"
        },
        {
            "Id": 1010422016,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 16,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "『ファンティー――』",
            "BackgroundCharacterImageFileName": "141920_0",
            "VoiceFileName": "10104220140",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422017,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 17,
            "SpeakerName": "カトリナ",
            "FontSize": 2,
            "Phrase": "『動くんじゃない！』",
            "BackgroundCharacterImageFileName": "141920_0",
            "VoiceFileName": "10104220150",
            "CharacterMotions": [],
            "SpeakerIconId": "103"
        },
        {
            "Id": 1010422018,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 18,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "（やば、何度も稽古で観てきてるのにグッときちゃうな……。/nそれにさっすーの演技……余計に響いてくる……）",
            "VoiceFileName": "10104220160",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
        {
            "Id": 1010422019,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 19,
            "FontSize": 2,
            "Phrase": "",
            "BackgroundImageFileName": "393",
            "BackgroundImageFileFadeType": 1,
