        },
        {
            "Id": 1010422004,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 4,
            "SpeakerName": "シリウス劇団員",
            "FontSize": 2,
            "Phrase": "『そこのお方、何を大声で喚きなすっている』",
            "VoiceFileName": "10104220030",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1010422005,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 5,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "『司祭様！子供を見かけませんでしたか！』",
            "VoiceFileName": "10104220040",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422006,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 6,
            "SpeakerName": "シリウス劇団員",
            "FontSize": 2,
            "Phrase": "『子供ですか？いいえ』",
            "VoiceFileName": "10104220050",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1010422007,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 7,
            "SpeakerName": "知冴",
            "FontSize": 2,
            "Phrase": "『プチ・ジェルヴェというこのくらいの少年で、サヴォアの者です』",
            "VoiceFileName": "10104220060",
            "CharacterMotions": [],
            "SpeakerIconId": "106"
        },
        {
            "Id": 1010422008,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 8,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "（心配、いらなさそうだな。/n最初っから飛ばしまくりだ）",
            "VoiceFileName": "10104220070",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
        {
            "Id": 1010422009,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 9,
            "SpeakerName": "ぱんだ",
            "FontSize": 2,
            "Phrase": "（でも、それでいいんだからな）",
            "VoiceFileName": "10104220080",
            "CharacterMotions": [],
            "SpeakerIconId": "105"
        },
        {
            "Id": 1010422010,
            "EpisodeMasterId": 1010422,
            "Order": 1,
            "GroupOrder": 10,
            "FontSize": 2,
            "Phrase": "",
