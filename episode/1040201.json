            "EpisodeMasterId": 1040201,
            "Order": 1,
            "GroupOrder": 21,
            "SpeakerName": "美兎",
            "FontSize": 2,
            "Phrase": "週末にお泊まり会して、正解だったね。/nみんなともっと仲良くなれた気がする。",
            "VoiceFileName": "10402010200",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 200,
                    "BodyMotionMasterId": 20,
                    "LipSyncMasterId": 1,
