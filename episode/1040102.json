            "SpeakerIconId": "401"
        },
        {
            "Id": 1040102024,
            "EpisodeMasterId": 1040102,
            "Order": 1,
            "GroupOrder": 21,
            "SpeakerName": "マチコ",
            "FontSize": 2,
            "Phrase": "一度諦めたのに、何故電姫に入団しようと考えた？",
