            "SpeakerIconId": "302"
        },
        {
            "Id": 2006608072,
            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 72,
            "SpeakerName": "銀河座幹部",
            "FontSize": 2,
            "Phrase": "もちろん、今すぐに明確な返事を求めているわけじゃない。/n君には君の事情がある。……ただ、考えておいて欲しい。",
            "VoiceFileName": "20066080700",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
