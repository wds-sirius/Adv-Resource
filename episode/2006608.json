            "VoiceFileName": "20066080460",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006608048,
            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 48,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "な……これは……。",
            "VoiceFileName": "20066080470",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 500,
                    "BodyMotionMasterId": 5,
                    "LipSyncMasterId": 4,
