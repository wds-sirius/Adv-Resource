            ],
            "SpeakerIconId": "301"
        },
        {
            "Id": 2006608022,
            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 22,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "（銀河座としてだけじゃない、千寿家の役者として築き上げたもの）",
            "VoiceFileName": "20066080230",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "BodyMotionMasterId": 4,
                    "SpineId": 30101,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "301"
        },
        {
            "Id": 2006608023,
            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 23,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "（背負ったものを考えれば、ここで資質を疑われるわけにはいかない。/n　……私では貴女を守れない、貴女のようになれないのです）",
            "VoiceFileName": "20066080240",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 73,
                    "SpineId": 30101,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "301"
        },
        {
