            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 68,
            "SpeakerName": "銀河座幹部",
            "FontSize": 2,
            "Phrase": "ああ、その名は……『星雲』だ。",
            "BgmFileName": "999",
            "VoiceFileName": "20066080660",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
