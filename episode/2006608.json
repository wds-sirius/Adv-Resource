            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 76,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "（それが今じゃどうだ、火鍋にワークショップ……、/n　そしてあの会議ではあの暦が庇ってくれた）",
            "VoiceFileName": "20066080740",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 71,
                    "BodyMotionMasterId": 15,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
