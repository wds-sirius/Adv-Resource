            "VoiceFileName": "20066080670",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 500,
                    "BodyMotionMasterId": 5,
                    "LipSyncMasterId": 3,
                    "SpineId": 30201,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
