            "SpeakerIconId": "301"
        },
        {
            "Id": 2006608021,
            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 21,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "（……やはり私の立場では、/n　ラモーナさんを火の粉から守るにも限度がある）",
            "VoiceFileName": "20066080210",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 400,
                    "SpineId": 30101,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
