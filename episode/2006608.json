            "SpeakerIconId": "302"
        },
        {
            "Id": 2006608070,
            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 70,
            "SpeakerName": "銀河座幹部",
            "FontSize": 2,
            "Phrase": "設立はまだ先になるが……私は座長を君に任せたいと思っている。/n勿論、私の名前は好きに使って良い。後援させてもらう。",
            "VoiceFileName": "20066080680",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2006608071,
            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 71,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "座長を、私に……。/nしかし……。",
            "VoiceFileName": "20066080690",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 400,
                    "BodyMotionMasterId": 25,
                    "LipSyncMasterId": 4,
                    "SpineId": 30201,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
