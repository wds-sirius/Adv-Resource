            "EpisodeMasterId": 2006608,
            "Order": 1,
            "GroupOrder": 73,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "（新たな劇団、それに私が座長……。/n　ダメだ、色々と頭が追いつかない）",
            "VoiceFileName": "20066080710",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 73,
                    "BodyMotionMasterId": 25,
                    "SpineId": 30201,
