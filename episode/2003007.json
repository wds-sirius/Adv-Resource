            "GroupOrder": 3,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "マジですか？　あざす！",
            "VoiceFileName": "20030070030",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 502,
                    "BodyMotionMasterId": 5,
                    "LipSyncMasterId": 4,
