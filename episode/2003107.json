            "SpeakerIconId": "106"
        },
        {
            "Id": 2003107016,
            "EpisodeMasterId": 2003107,
            "Order": 1,
            "GroupOrder": 15,
            "SpeakerName": "カトリナ",
            "FontSize": 2,
            "Phrase": "誰かを見て楽しそうに笑ってるわね。/nなら……ここなとの思い出……とか？",
            "VoiceFileName": "20031070140",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
