        },
        {
            "Id": 141411021,
            "EpisodeMasterId": 141411,
            "Order": 1,
            "GroupOrder": 20,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "色んな人の力借りて今のウチがいるワケだけど、/n和尚にも助けてもらったからさ。",
            "VoiceFileName": "1414110180",
            "CharacterMotions": [
                {
