            "SpeakerIconId": "403"
        },
        {
            "Id": 141411056,
            "EpisodeMasterId": 141411,
            "Order": 1,
            "GroupOrder": 54,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "まー、初めて電姫を観た時に感じたキラキラには敵わないかもだけど。",
            "VoiceFileName": "1414110520",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 105,
                    "BodyMotionMasterId": 6,
