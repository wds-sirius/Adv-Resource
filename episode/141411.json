        },
        {
            "Id": 141411015,
            "EpisodeMasterId": 141411,
            "Order": 1,
            "GroupOrder": 14,
            "SpeakerName": "和尚",
            "FontSize": 2,
            "Phrase": "それこそバトルになるぞ。/n饅頭に羊羹、お茶に合うモンは山ほどあるからな。",
            "VoiceFileName": "1414110120",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
