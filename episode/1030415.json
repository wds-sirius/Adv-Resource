            "EpisodeMasterId": 1030415,
            "Order": 1,
            "GroupOrder": 28,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "ひとまず今は、皆の受け皿になれればと思ってな。",
            "VoiceFileName": "10304150280",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 100,
                    "BodyMotionMasterId": 15,
                    "LipSyncMasterId": 1,
                    "SpineId": 30201,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                },
                {
                    "slotNumber": 2,
