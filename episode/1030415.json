            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1030415022,
            "EpisodeMasterId": 1030415,
            "Order": 1,
            "GroupOrder": 22,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
