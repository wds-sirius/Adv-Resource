            "EpisodeMasterId": 1030415,
            "Order": 1,
            "GroupOrder": 27,
            "SpeakerName": "緋花里",
            "FontSize": 2,
            "Phrase": "でもラモさん、ワークショップは禁止になったんだよね？/n何してたの？",
            "VoiceFileName": "10304150270",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
