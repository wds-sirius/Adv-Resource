            "EpisodeMasterId": 1030415,
            "Order": 1,
            "GroupOrder": 24,
            "SpeakerName": "ラモーナ",
            "FontSize": 2,
            "Phrase": "……ん？　緋花里、帰って来てたのか！/n気づかなくてすまない。遅かったな。",
            "VoiceFileName": "10304150240",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 502,
                    "BodyMotionMasterId": 15,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
