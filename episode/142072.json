        },
        {
            "Id": 142072020,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 20,
            "SpeakerName": "蕾",
            "FontSize": 2,
            "Phrase": "わっ、こんな近くに……！/nあはは、見えてるかな？　私、蕾お姉ちゃんだよー？",
