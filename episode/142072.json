        {
            "Id": 142072022,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 22,
            "SpeakerName": "蕾の母",
            "FontSize": 2,
            "Phrase": "こらこら、ダメよ。そんなところ触ったら壊れちゃうわ。/nほら、そろそろお母さんのところに戻りましょうね。",
            "VoiceFileName": "1420720210",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 142072023,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 23,
            "SpeakerName": "蕾",
            "FontSize": 2,
            "Phrase": "あの子、可愛いね。/nなんか私まで癒されちゃった。",
            "VoiceFileName": "1420720220",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 200,
                    "BodyMotionMasterId": 20,
                    "LipSyncMasterId": 2,
                    "SpineId": 40401,
