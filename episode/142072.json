        },
        {
            "Id": 142072006,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 6,
            "SpeakerName": "蕾の母",
            "FontSize": 2,
            "Phrase": "本当？　あ、そういえば！　この間テレビで劇団の特集、ちょっと見たわよ！/nあれに映ってたの、蕾じゃないの？",
            "VoiceFileName": "1420720050",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 142072007,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 7,
            "SpeakerName": "蕾",
            "FontSize": 2,
            "Phrase": "うん、たまたまね。/nちょっとだけ、映ってた。",
            "VoiceFileName": "1420720060",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 105,
                    "BodyMotionMasterId": 13,
