            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 142072015,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 15,
            "SpeakerName": "蕾の母",
            "FontSize": 2,
            "Phrase": "従姉の栞ちゃんの子よ。女の子ですって。",
            "VoiceFileName": "1420720140",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 142072016,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 16,
            "SpeakerName": "蕾の母",
            "FontSize": 2,
            "Phrase": "栞ちゃんは他の親戚に挨拶してるから代わりにお世話してるんだけど、/nこの子ったらもう、元気いっぱいで……。",
            "VoiceFileName": "1420720150",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 142072017,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 17,
            "SpeakerName": "赤ちゃん",
            "FontSize": 2,
            "Phrase": "だーっ！",
