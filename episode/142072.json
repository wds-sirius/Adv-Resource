        },
        {
            "Id": 142072018,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 18,
            "SpeakerName": "蕾",
            "FontSize": 2,
            "Phrase": "わあっ、ほんとだ。すごい元気だね。/nそんなにカメラが珍しいのかな？",
