        },
        {
            "Id": 142072005,
            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 5,
            "SpeakerName": "蕾",
            "FontSize": 2,
            "Phrase": "久しぶり。……ごめんね、なかなか連絡できなくて。/n私は変わらず元気にしてるよ。",
            "VoiceFileName": "1420720040",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
