            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 41,
            "SpeakerName": "蕾",
            "FontSize": 2,
            "Phrase": "うん、ありがとう。……じゃあ、またね。",
            "VoiceFileName": "1420720380",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "HeadMotionMasterId": 1,
                    "BodyMotionMasterId": 13,
                    "LipSyncMasterId": 1,
                    "SpineId": 40401,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
