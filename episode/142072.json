            "EpisodeMasterId": 142072,
            "Order": 1,
            "GroupOrder": 35,
            "SpeakerName": "蕾",
            "FontSize": 2,
            "Phrase": "……そうだね。/n今日はありがとう、繋いでくれて。",
            "VoiceFileName": "1420720320",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 20,
                    "LipSyncMasterId": 1,
