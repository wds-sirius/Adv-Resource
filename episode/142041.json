        },
        {
            "Id": 142041008,
            "EpisodeMasterId": 142041,
            "Order": 1,
            "GroupOrder": 8,
            "SpeakerName": "演出チーフ",
            "FontSize": 2,
            "Phrase": "もういい！　全員、一旦休憩だ！",
            "VoiceFileName": "1420410060",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
