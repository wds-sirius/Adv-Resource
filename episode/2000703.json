        {
            "Id": 2000703038,
            "EpisodeMasterId": 2000703,
            "Order": 1,
            "GroupOrder": 37,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "上層部から寄せられた期待に応えるため、/n舞台を成功させるため、舞台に立ち続けて……。",
            "VoiceFileName": "20007030330",
            "CharacterMotions": [],
            "SpeakerIconId": "301"
